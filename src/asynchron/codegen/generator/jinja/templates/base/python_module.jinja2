{# @formatter:off #}
{% if meta.ignore_formatter %}
# @formatter:off
{% endif %}
{% if meta.enable_meta_doc %}
"""
This module was generated by {{ meta.generator_name }} application, see {{ meta.generator_link }}

Config          {{ meta.config_path }}
Created at      {{ meta.generator_started_at|datetime_iso }}
Author          {{ meta.author }}

    {% if module.description %}
{{ module.description }}
    {% endif %}
"""

{% endif %}
{% block imports %}
    {% for import in imports|unique|sorted("path") %}
        {% if import.path != ("builtins",) %}
import {{ import.path|join(".") }}{% if import.alias %} as {{ import.alias }}{% endif %}

        {% endif %}
    {% endfor %}
{% endblock %}


{% block constants %}
    {% for constant in constants|unique|sorted("path") %}
{{ constant.path|constcase }} = {{ constant.expr.value }}
    {% endfor %}
{% endblock %}


{% block classes %}
    {% for class in classes|unique|sorted("path") %}
class {{ class.path|pascalcase }}:
    pass
    {% endfor %}
{% endblock %}


{% block functions %}
    {% for function in functions|unique|sorted("path") %}
def {{ function.path|snakecase }}():
    raise NotImplementedError
    {% endfor %}
{% endblock %}


{% block main %}
{% endblock %}


{% if meta.ignore_formatter %}
# @formatter:on
{% endif %}
{# @formatter:on #}
